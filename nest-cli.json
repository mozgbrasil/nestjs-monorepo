{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-monorepo/src",
  "monorepo": true,
  "root": "apps/nestjs-monorepo",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nestjs-monorepo/tsconfig.app.json"
  },
  "projects": {
    "nestjs-monorepo": {
      "type": "application",
      "root": "apps/nestjs-monorepo",
      "entryFile": "main",
      "sourceRoot": "apps/nestjs-monorepo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjs-monorepo/tsconfig.app.json"
      }
    },
    "ecommerce-app": {
      "type": "application",
      "root": "apps/ecommerce-app",
      "entryFile": "main",
      "sourceRoot": "apps/ecommerce-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ecommerce-app/tsconfig.app.json"
      }
    }
  }
}